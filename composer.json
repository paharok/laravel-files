{
    "name": "paharok/laravel-files",
    "description": "Filemanager for Laravel",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "paharok",
            "email": "soltanovka@gmail.com",
            "homepage": "https://github.com/paharok"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "intervention/image": "2.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Paharok\\Laravelfiles\\Providers\\LaraveFilesServiceProvider"
            ],
            "aliases": {
                "ChangeImage": "Paharok\\Laravelfiles\\Helpers\\ChangeImageIntervention"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Paharok\\Laravelfiles\\": "src"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan vendor:publish --tag=laravel-files-assets --force"
        ]
    }
}
